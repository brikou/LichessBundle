<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="lichess.critic.user.class">Bundle\LichessBundle\Critic\UserCritic</parameter>
    </parameters>

    <services>

        <service id="lichess.critic.user" class="%lichess.critic.user.class%">
            <argument type="service" id="lichess.repository.game" />
            <argument type="service" id="fos_user.repository.user" />
        </service>

    </services>

</container>
