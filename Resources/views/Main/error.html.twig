{% extends "LichessBundle:Main:baseError.html.twig" %}
{% block error_title %}{% trans 'Server error' %} ({{ code }}){% endblock %}
{% block error_text %}{% trans "Lichess was unable to process your request. This may be due to maintenance. Please try again soon, or contact me if the problem persists:" %}{% endblock %}
{% block error_next %}<a class="lichess_new_game" href="{{ url }}">{% trans 'Try again' %}</a>{% endblock %}
